<?xml version="1.0" encoding="UTF-8"?>
<cq:componentQuery xmlns:cq="http://www.eclipse.org/buckminster/CQuery-1.0" resourceMap="amp-platform.rmap" properties="local.properties">
    <cq:rootRequest name="org.eclipse.amp.releng" componentType="buckminster"/>
    <cq:property key="target.arch" value="*"/>
    <cq:property key="target.os" value="*"/>
    <cq:property key="target.ws" value="*"/>
    <cq:advisorNode namePattern="^org\.apache([\.].+)+\.source$" skipComponent="true"/>
    <cq:advisorNode namePattern="de\.itemis\.xtext\.antlr" componentType="osgi.bundle" skipComponent="true"/>
</cq:componentQuery>
